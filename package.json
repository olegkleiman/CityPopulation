{
  "name": "CityMovements",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "scripts": {
    "prepare": "geojson-merge ./data/geoJson/*.geojson > ./dist/data/merged.json",
